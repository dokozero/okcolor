{
  "name": "okcolor",
  "version": "9.0",
  "description": "A Figma plugin to get OK Colors",
  "scripts": {
    "build-plugin": "vite build --config vite-plugin.config.ts",
    "build-ui": "vite build --config vite-ui.config.ts",
    "watch-plugin": "vite build --watch --config vite-plugin.config.ts",
    "watch-ui": "vite build --watch --config vite-ui.config.ts",
    "build": "pnpm run build-plugin && pnpm run build-ui"
  },
  "dependencies": {
    "@nanostores/react": "^0.7.1",
    "culori": "^3.2.0",
    "nanostores": "^0.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "twgl.js": "^5.5.3",
    "vite-plugin-virtual-plain-text": "^1.4.3"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.79.0",
    "@types/culori": "^2.0.1",
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-singlefile": "^0.13.5"
  }
}
