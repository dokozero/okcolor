{
  "name": "okcolor",
  "description": "A Figma plugin to get OK Colors",
  "author": "Doko Zero",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "tests": "vitest run",
    "eslint-types": "pnpm exec eslint \"./src/types.ts\"",
    "eslint-constants": "pnpm exec eslint \"./src/constants.ts\"",
    "eslint-backend-helpers": "pnpm exec eslint \"./src/backend/helpers/**/*\"",
    "eslint-backend-main": "pnpm exec eslint \"./src/backend/main.ts\"",
    "eslint-ui-components": "pnpm exec eslint \"./src/ui/components/**/*\"",
    "eslint-ui-helpers": "pnpm exec eslint \"./src/ui/helpers/**/*\"",
    "eslint-ui-stores": "pnpm exec eslint \"./src/ui/stores/**/*\"",
    "eslint-ui-app": "pnpm exec eslint \"./src/ui/app.tsx\"",
    "eslint-checks": "pnpm eslint-types & pnpm eslint-constants & pnpm eslint-backend-helpers & pnpm eslint-backend-main & pnpm eslint-ui-components & pnpm eslint-ui-helpers & pnpm eslint-ui-stores & pnpm eslint-ui-app",
    "type-backend-checks": "tsc --noEmit --project ./src/backend/tsconfig.json",
    "type-ui-checks": "tsc --noEmit --project ./src/ui/tsconfig.json",
    "all-checks": "pnpm eslint-checks && pnpm type-backend-checks && pnpm type-ui-checks",
    "build-backend": "vite build --config vite-backend.config.ts",
    "build-ui": "vite build --config vite-ui.config.ts",
    "watch-backend": "vite build --watch --config vite-backend.config.ts",
    "watch-ui": "vite build --watch --config vite-ui.config.ts",
    "build": "pnpm build-backend & pnpm build-ui"
  },
  "dependencies": {
    "@nanostores/logger": "^1.0.0",
    "@nanostores/react": "^1.0.0",
    "@types/apca-w3": "^0.1.3",
    "@types/lodash": "^4.17.16",
    "apca-w3": "^0.1.9",
    "culori": "^4.0.1",
    "lodash": "^4.17.21",
    "nanostores": "^0.9.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "twgl.js": "^6.1.1",
    "vite-plugin-virtual-plain-text": "^1.4.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@figma/plugin-typings": "^1.110.0",
    "@types/culori": "^2.1.1",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-plugin-singlefile": "^2.2.0",
    "vitest": "^3.1.2"
  }
}
